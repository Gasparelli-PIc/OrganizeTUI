{
    "name": "organize_tui",
    "version": "1.0.0",
    "description": "Organize files in a directory",
    "license": "MIT",
    "author": "Gabriel Santos Costa",
    "type": "module",
    "main": "src/index.js",
    "scripts": {
        "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint . --config .config/.eslintrc.cjs",
        "lint:fix": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint . --fix --config .config/.eslintrc.cjs",
        "start": "node src/index.js",
        "build": "esbuild src/index.js --bundle --platform=node --target=node18 --format=cjs --outfile=dist/bundle.cjs",
        "format": "prettier --config .config/.prettierrc --ignore-path .config/.prettierignore --write .",
        "exe": "bun build ./src/index.js --compile --outfile dist/meu-app-cli.exe"
    },
    "dependencies": {
        "@inquirer/prompts": "^8.1.0",
        "fs-extra": "^11.3.2",
        "inquirer": "^13.1.0"
    },
    "devDependencies": {
        "cross-env": "^10.1.0",
        "esbuild": "^0.27.1",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "globals": "^16.5.0",
        "prettier": "^3.7.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Gasparelli-PIc/OrganizeTUI.git"
    },
    "keywords": [
        "Organize"
    ],
    "bugs": {
        "url": "https://github.com/Gasparelli-PIc/OrganizeTUI/issues"
    },
    "homepage": "https://github.com/Gasparelli-PIc/OrganizeTUI#readme"
}
